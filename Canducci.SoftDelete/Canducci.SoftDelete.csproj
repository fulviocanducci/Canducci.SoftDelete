<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netstandard2.1;netcoreapp3.1</TargetFrameworks>
    <LangVersion>8</LangVersion>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Authors>Canducci</Authors>
    <Company>Canducci</Company>
    <Description>Version 1.1.1 SoftDelete (char, bool and DateTime?)</Description>
    <PackageIcon>remove.png</PackageIcon>
    <RepositoryUrl>https://github.com/fulviocanducci/Canducci.SoftDelete</RepositoryUrl>
    <RepositoryType>GIT</RepositoryType>
    <PackageTags>EntityFrameworkCore ORM NetCore Entity EntityFramework</PackageTags>
    <PackageProjectUrl>https://github.com/fulviocanducci/Canducci.SoftDelete</PackageProjectUrl>
    <Version>1.1.1</Version>
    <PackageReleaseNotes>Version 1.1.1 SoftDelete (char, bool and DateTime?)</PackageReleaseNotes>
  </PropertyGroup>

  <ItemGroup Condition="'$(TargetFramework)' == 'netstandard2.1'">
    <PackageReference Include="Microsoft.EntityFrameworkCore">
      <Version>5.0.0-rc.1.20451.13</Version>
    </PackageReference>
  </ItemGroup>

  <ItemGroup Condition="'$(TargetFramework)' == 'netcoreapp3.1'">
    <PackageReference Include="Microsoft.EntityFrameworkCore">
      <Version>5.0.0-rc.1.20451.13</Version>
    </PackageReference>
  </ItemGroup>

  <ItemGroup>
    <None Include="remove.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
